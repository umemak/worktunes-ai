{
  "name": "worktunes-api-worker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "esbuild src/index.ts --bundle --outdir=dist --format=esm --platform=browser",
    "db:migrate": "wrangler d1 execute worktunes-db --file=./schema.sql",
    "db:local": "wrangler d1 execute worktunes-db --local --file=./schema.sql"
  },
  "dependencies": {
    "hono": "^3.11.7",
    "jose": "^5.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "wrangler": "^3.22.1",
    "esbuild": "^0.19.11",
    "typescript": "^5.3.0"
  }
}
